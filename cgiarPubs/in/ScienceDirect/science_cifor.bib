number = "3",
pages = "217 - 223",
year = "2000",
note = "",
issn = "1369-5266",
doi = "http://dx.doi.org/10.1016/S1369-5266(00)80068-0",
url = "http://www.sciencedirect.com/science/article/pii/S1369526600800680",
