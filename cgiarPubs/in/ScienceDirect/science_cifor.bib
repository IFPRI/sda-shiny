doi = "http://dx.doi.org/10.1016/j.ibiod.2015.08.023",
url = "http://www.sciencedirect.com/science/article/pii/S0964830515300780",
author = "Raquel Martín-Sampedro and Úrsula Fillat and David Ibarra and María E. Eugenio",
keywords = "Endophytes",
keywords = "Ascomycetes",
