url = "http://www.sciencedirect.com/science/article/pii/S0378112715003242",
author = "Hortensia Sixto and Isabel Cañellas and Joost van Arendonk and Pilar Ciria and Francesc Camps and Mario Sánchez and Mariola Sánchez-González",
keywords = "Populus deltoides",
