author = "M. van Noordwijk",
keywords = "Agroforestry",
keywords = "Bioenergy",
keywords = "Biofuel",
keywords = "Carbon markets",
keywords = "Diets",
keywords = "Footprint",
keywords = "Greenhouse gases",
keywords = "Leakage",
keywords = "Lifestyles",
keywords = "Methane",
keywords = "Nitrous oxide",
keywords = "Peatlands",
keywords = "Rice paddies",
keywords = "Ruminants",
keywords = "Soil carbon ",
abstract = "Abstract Reducing recurrent greenhouse gas emissions of agriculture requires increases in production per unit area simultaneously with (1) reducing temporary excesses of available nitrogen that induce high \{N2O\} emissions, (2) reducing \{CH4\} emissions by better feeding regimes of ruminants and less inundation of rice-fields, (3) wiser use of peatlands reducing \{CO2\} emissions, and (4) better waste recycling. (5) Increased carbon storage in soils, and (6) trees in the agricultural landscape (agroforestry) also helps. Renewed bioenergy dependency increases agricultural emissions under current accounting rules. Shifts in lifestyles and human diets are needed to reconcile population size, desired wellbeing, and footprints of production systems. "
}
@article{Sunderlin2001767,
title = "Economic Crisis, Small Farmer Well-Being, and Forest Cover Change in Indonesia ",
journal = "World Development ",
volume = "29",
number = "5",
pages = "767 - 782",
year = "2001",
note = "",
issn = "0305-750X",
doi = "http://dx.doi.org/10.1016/S0305-750X(01)00009-2",
url = "http://www.sciencedirect.com/science/article/pii/S0305750X01000092",
author = "William D. Sunderlin and Arild Angelsen and Daju Pradnja Resosudarmo and Ahmad Dermawan and Edy Rianto",
keywords = "economic crisis",
keywords = "deforestation",
keywords = "agriculture",
keywords = "aid",
keywords = "Asia",
keywords = "Indonesia ",
abstract = "Field research was conducted on 1,050 Indonesian households to understand the effects of the Asian economic crisis on the well-being of small farmers outside of Java and on their forest-clearing practices. The main findings are: (a) most farmers perceived themselves as worse off during the crisis than before, challenging the claim that farmers with export income would be better off and (b) forest clearing by farmers increased significantly during the crisis to expand rubber holdings and other tree crops, with the aim of increasing future income security. Among the policy lessons are that crop diversification and targeted aid can minimize impoverishment and avert increased forest clearing following macroeconomic destabilization. "
}
@article{Belcher20051435,
title = "Global patterns and trends in the use and management of commercial NTFPs: Implications for livelihoods and conservation ",
