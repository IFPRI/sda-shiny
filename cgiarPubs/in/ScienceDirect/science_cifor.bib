number = "",
pages = "S29 - S42",
year = "2014",
note = "Forests, Livelihoods, and Conservation ",
issn = "0305-750X",
doi = "http://dx.doi.org/10.1016/j.worlddev.2014.03.005",
url = "http://www.sciencedirect.com/science/article/pii/S0305750X14000710",
author = "Sven Wunder and Jan Börner and Gerald Shively and Miriam Wyman",
keywords = "livelihoods",
keywords = "risk",
keywords = "shocks",
keywords = "coping strategies",
keywords = "income",
keywords = "diversification ",
abstract = "Summary In the forest–livelihoods literature, forests are widely perceived to provide both common safety nets to shocks and resources for seasonal gap-filling. We use a large global-comparative dataset to test these responses. We find households rank forest-extraction responses to shocks lower than most common alternatives. For seasonal gap-filling, forest extraction also has limited importance. The minority of households using forests for coping is asset-poor and lives in villages specialized on forests, in particular timber extraction. Overall, forest resources may be less important as a buffer between agricultural harvests and in times of unforeseen hardship than has been found in many case studies. "
}
@article{Abebaw201272,
title = "Dry forest based livelihoods in resettlement areas of Northwestern Ethiopia ",
journal = "Forest Policy and Economics ",
volume = "20",
