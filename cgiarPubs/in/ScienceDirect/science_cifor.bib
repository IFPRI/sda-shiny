url = "http://www.sciencedirect.com/science/article/pii/S1095643300001628",
author = "Marcy N. Wilder and Do Thi Thanh Huong and Muharijadi Atmomarsono and Tran Thi Thanh Hien and Truong Quoc Phu and Wei-Jun Yang",
keywords = "Enzymatic activity",
