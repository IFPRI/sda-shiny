url = "http://www.sciencedirect.com/science/article/pii/S0098847212001943",
author = "David Sánchez-Gómez and T. Matthew Robson and Antonio Gascó and Eustaquio Gil-Pelegrín and Ismael Aranda",
keywords = "Climate change",
