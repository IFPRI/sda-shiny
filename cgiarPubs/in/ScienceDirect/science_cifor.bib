@article{Tang2016195,
title = "Effect of fresh aluminum hydroxide gels on algae removal from micro-polluted water by polyaluminum chloride coagulant ",
journal = "Journal of the Taiwan Institute of Chemical Engineers ",
volume = "63",
number = "",
pages = "195 - 201",
year = "2016",
note = "",
issn = "1876-1070",
