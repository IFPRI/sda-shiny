volume = "3",
number = "",
pages = "78 - 84",
year = "2015",
note = "",
issn = "2352-3409",
doi = "http://dx.doi.org/10.1016/j.dib.2015.01.008",
url = "http://www.sciencedirect.com/science/article/pii/S2352340915000104",
author = "Anne-Catherine Vanhove and Wesley Vermaelen and Alberto Cenci and Rony Swennen and Sebastien C. Carpentier",
