number = "3",
pages = "381 - 386",
year = "2008",
note = "",
issn = "0045-6535",
doi = "http://dx.doi.org/10.1016/j.chemosphere.2007.07.004",
url = "http://www.sciencedirect.com/science/article/pii/S0045653507008739",
author = "L. Lhomme and S. Brosillon and D. Wolbert",
keywords = "Photocatalysis",
keywords = "Pesticides",
keywords = "Commercial pesticides solutions",
keywords = "Waste water treatment ",
abstract = "Heterogeneous photocatalysis of pesticides is an effective process for removing pesticides from pure water. With a view to treating real agricultural effluents, this paper deals with the degradation of the chlortoluron and cyproconazole pesticides in pure water and the treatment of commercial solutions by photocatalysis on TiO2 coated media. The process was effective in degrading and mineralizing the pesticides. The changes of the fate of heteroatoms showed that during irradiation of the chlortoluron and cyproconazole, \{NH\} 4 + and \{NO\} 3 - ions were produced. A release of chloride ions was observed from the beginning of the irradiation and stoichiometry was achieved. The photodegradation of chlortoluton and cyproconazole in commercial solutions was studied. For the degradation of chlortoluton in a commercial solution, the mineralization was completely achieved whereas in the case of the commercial cyproconazole solution, the degradation kinetic was lower. These results highlight the fact that the chemical nature of the additives in the commercial pesticide solutions does significantly affect the degradation yield of the target compound by photocatalysis. "
}
@incollection{Audic2007369,
title = "Chapter 20 - Biodegradable Material Obtained from Renewable Resource: Plasticized Sodium Caseinate Films ",
editor = "Letcher, Trevor M. ",
booktitle = "Thermodynamics, Solubility and Environmental Issues ",
publisher = "Elsevier",
edition = "",
address = "Amsterdam",
year = "2007",
pages = "369 - 382",
isbn = "978-0-444-52707-3",
doi = "http://dx.doi.org/10.1016/B978-044452707-3/50022-7",
url = "http://www.sciencedirect.com/science/article/pii/B9780444527073500227",
author = "Jean-Luc Audic and Florence Fourcade and Bernard Chaufer",
abstract = "Publisher Summary This chapter focuses on plasticized sodium caseinate films. Sodium caseinate (NaCAS) is a commercially available protein obtained by acid precipitation of casein, which is the main protein in cow's milk. This protein is obtained with good purity (up to 90%), and its thermoplastic and film-forming properties are because of its random coil nature and its ability to form weak intermolecular interactions. Such films have good elongation at break points (from 150 to ∼400%) and a rather low tensile strength ranging from 20 to 30 MPa. However, compared with synthetic films, casein-based films have two major drawbacks: poor mechanical properties such as lower tensile strength and elongation at break point and a high water sensitivity in the form of high water solubility and water vapor permeability. A chemical crosslinking of NaCAS with \{HCHO\} increases the tensile properties and overcomes the water sensitivity problem of the protein film. The specific solubility of NaCAS obtained from a \{UV\} absorbance method shows that the improvement in the NaCAS film water resistance is closely related to the cross-linker content. However, it is also observed that during water immersion, the triethanolamine (TEA) plasticizer extensively migrates out of the films to the surrounding solution, irrespective of the cross-linker-to-reactive-groups ratio. "
}
@article{ValdiviaDíaz201564,
title = "Local Knowledge of Native Potato (Solanum spp) for Long-term Monitoring on three Andean Communities of Apurimac, Peru ",
