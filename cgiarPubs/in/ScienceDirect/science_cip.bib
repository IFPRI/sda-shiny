@article{delCarpioPerochena20111134,
title = "Biofilm Dissolution and Cleaning Ability of Different Irrigant Solutions on Intraorally Infected Dentin ",
journal = "Journal of Endodontics ",
volume = "37",
number = "8",
pages = "1134 - 1138",
year = "2011",
note = "",
issn = "0099-2399",
doi = "http://dx.doi.org/10.1016/j.joen.2011.04.013",
url = "http://www.sciencedirect.com/science/article/pii/S0099239911004924",
author = "Aldo E. del Carpio-Perochena and Clovis M. Bramante and Marco A.H. Duarte and Bruno Cavalini Cavenago and Marcelo H. Villas-Boas and Marcia S. Graeff and Norberti Bernardineli and Flaviana Bombarda de Andrade and Ronald Ordinola-Zapata",
keywords = "Acridine orange",
keywords = "bacteria",
keywords = "confocal laser scanning microscopy",
keywords = "dentin",
keywords = "irrigant solutions",
keywords = "oral biofilms ",
abstract = "Introduction The aim of this study was to evaluate the biofilm dissolution and cleaning ability of different irrigant solutions on intraorally infected dentin. Methods One hundred twenty bovine dentin specimens were infected intraorally by using a removable orthodontic device. Thirty samples were used for each irrigant solution: 2% chlorhexidine and 1%, 2.5%, and 5.25% sodium hypochlorite (NaOCl). The solutions were used for 5, 15, and 30 minutes and at 2 experimental volumes, 500 μL and 1 mL. The samples were stained by using acridine orange dye before and after the experiments and evaluated by using a confocal microscope. The percentage of biofilm, isolated cells, and noncolonized dentin was measured by using a grid system. Differences in the reduction or increase of the studied parameters were assessed by using nonparametric methods (P &lt; .05). Results The higher values of biofilm dissolution and noncolonized dentin were found in the 30-minute NaOCl group and in the 5-minute and 15-minute groups of 5.25% NaOCL. The use of 2% chlorhexidine solution did not improve the biofilm dissolution or increase the cleaning of the dentin in comparison with the NaOCl solutions (P &lt; .05). Conclusions Two percent chlorhexidine does not dissolve the biofilms. Thirty minutes of NaOCl are necessary to have higher values of biofilm dissolution and to increase the cleaning of the dentin independently of the concentration in comparison with the 5-minute and 15-minute contact times. "
}
@article{Gervais2008163,
