@article{Erenstein20081,
title = "Zero tillage impacts in India's rice–wheat systems: A review ",
journal = "Soil and Tillage Research ",
volume = "100",
number = "1–2",
pages = "1 - 14",
year = "2008",
note = "",
issn = "0167-1987",
doi = "http://dx.doi.org/10.1016/j.still.2008.05.001",
url = "http://www.sciencedirect.com/science/article/pii/S0167198708000822",
author = "Olaf Erenstein and Vijay Laxmi",
keywords = "Zero tillage",
keywords = "Direct drilling",
keywords = "Wheat",
keywords = "Impact",
keywords = "Rice–wheat system",
keywords = "Indo-Gangetic Plains ",
abstract = "To date, the most widely adopted resource conserving technology in the Indo-Gangetic Plains (IGP) of South Asia has been zero-tillage (ZT) wheat after rice, particularly in India. The paper reviews and synthesizes the experience with \{ZT\} in the Indian IGP. \{ZT\} wheat is particularly appropriate for rice–wheat systems in the \{IGP\} by alleviating system constraints by allowing earlier wheat planting, helping control the weed Phalaris minor, reducing production costs and saving water. \{ZT\} wheat after rice generates substantial benefits at the farm level (US$97 ha−1) through the combination of a ‘yield effect’ (a 5–7% yield increase, particularly due to more timely planting of wheat) and a ‘cost savings effect’ (US$52 ha−1, particularly tillage savings). These benefits explain the widespread interest of farmers and the rapidity of the diffusion across the Indian IGP, further aided by the wide applicability of this mechanical innovation. "
}
@article{Rice19981625,
title = "Farmers' use of improved seed selection practices in Mexican maize: Evidence and issues from the Sierra de Santa Marta ",
journal = "World Development ",
volume = "26",
number = "9",
pages = "1625 - 1640",
year = "1998",
note = "",
issn = "0305-750X",
doi = "http://dx.doi.org/10.1016/S0305-750X(98)00079-5",
url = "http://www.sciencedirect.com/science/article/pii/S0305750X98000795",
author = "Elizabeth Rice and Melinda Smale and José-Luis Blanco",
keywords = "genetic diversity",
keywords = "genetic resources",
keywords = "maize",
keywords = "participatory plant breeding",
keywords = "seed ",
abstract = "Participatory crop improvement strategies have been proposed as incentives for farmers to continue growing traditional varieties identified as important genetic resources. This pilot study among indigenous farmers in the Sierra de Santa Marta, Mexico, raises questions about the benefits of such strategies. Genetic gains may not be realized, given the high rates of seed replacement for the same varieties. Seed selection is more complex and women are more involved than was previously thought. Models developed to explain farmers' decisions will need to consider the interactions among households, and the factors affecting intravariety as well as intervariety choice. "
}
@article{Mottaleb2015557,
title = "Intra-Household Resource Allocation under Negative Income Shock: A Natural Experiment ",
journal = "World Development ",
volume = "66",
number = "",
pages = "557 - 571",
year = "2015",
note = "",
issn = "0305-750X",
doi = "http://dx.doi.org/10.1016/j.worlddev.2014.09.012",
url = "http://www.sciencedirect.com/science/article/pii/S0305750X14002770",
author = "Khondoker A. Mottaleb and Samarendu Mohanty and Ashok K. Mishra",
keywords = "cyclone",
keywords = "income shocks",
keywords = "farm household",
keywords = "gender-differentiated impacts",
keywords = "intra-household resource allocation ",
abstract = "Summary Using households from coastal districts in Bangladesh, as a case, and applying the difference-in-difference estimation method, this paper demonstrates a gender-differentiated outcome of negative income shocks for education expenditures of households. The cyclonic disaster that reduced crop income substantially increased the demand for labor as well as wages for rebuilding damaged farms. This in turn increased the opportunity costs of boys’ schooling, as reconstruction is a male-friendly sector. Consequently, parents withdrew their sons from school and engaged them in their households’ repair work and/or in wage-earning activities. However, girls’ schooling expenditure was unchanged in the affected farm and non-farm households. "
}
@article{ZHOU2011166,
title = "A \{SSR\} Linkage Map of Maize×Teosinte \{F2\} Population and Analysis of Segregation Distortion ",
