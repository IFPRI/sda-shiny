url = "http://www.sciencedirect.com/science/article/pii/S0378429008002487",
author = "Fahong Wang and Zhonghu He and Ken Sayre and Shengdong Li and Jisheng Si and Bo Feng and Lingan Kong",
keywords = "Conservation agriculture",
