keywords = "Kenya ",
abstract = "Summary This paper explores the link between the gender of a household head and food security in rural Kenya. The results show that the food security gap between male-headed households (MHHs) and female-headed households (FHHs) is explained by their differences in observable and unobservable characteristics. FHHs’ food security status would have been higher than it is now if the returns (coefficients) on their observed characteristics had been the same as the returns on the MHHs’ characteristics. Even if that had been the case, however, results indicate that \{FHHs\} would still have been less food-secure than the \{MHHs\} due to unobservable characteristics. "
}
@article{Ransom1989115,
title = "Effect of yellow-berry on field emergence of spring wheat ",
