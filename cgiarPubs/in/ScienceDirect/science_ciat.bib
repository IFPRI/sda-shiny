url = "http://www.sciencedirect.com/science/article/pii/S0341816214002124",
author = "Ngonidzashe Chirinda and Svenja Doreen Roncossek and Goswin Heckrath and Lars Elsgaard and Ingrid Kaag Thomsen and Jørgen Eivind Olesen",
keywords = "Root biomass",
