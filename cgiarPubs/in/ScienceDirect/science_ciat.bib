url = "http://www.sciencedirect.com/science/article/pii/S1049964402001809",
author = "Brigitte Dorn and Letizia Mattiacci and Anthony C Bellotti and Silvia Dorn",
keywords = "Aenasius vexans",
keywords = "Acerophagus coccois",
keywords = "Phenacoccus herreni",
keywords = "Biological control",
