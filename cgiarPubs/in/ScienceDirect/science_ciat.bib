@article{Clark198537,
title = "Transgressive yielding in bean: Maize intercrops; interference in time and space ",
journal = "Field Crops Research ",
volume = "11",
number = "",
pages = "37 - 53",
year = "1985",
note = "Pearl millet ",
issn = "0378-4290",
doi = "http://dx.doi.org/10.1016/0378-4290(85)90090-5",
url = "http://www.sciencedirect.com/science/article/pii/0378429085900905",
author = "E.Ann Clark and C.A. Francis",
abstract = "Patterns of dry matter and nitrogen accumulation and leaf area establishment were compared in moncrops and intercrops of indeterminate bush and climbing beans (Phaseolus vulgaris L.) with maize (Zea mays L.). The bean and maize growth cycles were found to be offset in time, with bean growth significantly exceeding maize growth during the first 26 days. Maize dry matter (DM) significantly exceeded that of the beans after 47 days but nitrogen content (N) did not differ consistently between the bean and maize monocrops after 33 days. The staggering of growth cycles in the bean: maize intercrops resulted in significantly more DM, N, and \{LAI\} then either component monocrop during all or part of the first 82 days, yet average intercrops yields of crude protein, energy and photosynthate equivalent did not exceed those of the higher yielding monocrop. Yield in monocrops and intercrops is discussed in relation to the temporal and spatial overlap in resource demands by the bean and maize. Potential predictors of ecological combining ability in simultaneously planted bean: maize intercrops are considered. "
}
@article{Clark1985151,
title = "Bean-maize intercrops: A comparison of bush and climbing bean growth habits ",
journal = "Field Crops Research ",
volume = "10",
number = "",
pages = "151 - 166",
year = "1985",
note = "",
issn = "0378-4290",
doi = "http://dx.doi.org/10.1016/0378-4290(85)90023-1",
url = "http://www.sciencedirect.com/science/article/pii/0378429085900231",
author = "E.Ann Clark and C.A. Francis",
abstract = "Indeterminate bush and climbing beans (Phaseolus vulgaris L.) were grown both in monoculture and intercropped with maize (Zea mays L.). Bean responses were contrasted to determine, first, the basis for the greater yield potential of the climibing beans in monoculture, and second, the source of the relatively larger yield losses exhibited by the climbing compared to the bush bean, when intercropped with maize. Climbing bean yield potential was associated with a longer life cycle than bush bean (92 vs. 80 days to maturity), and with prolonged durations of both leaf area establishment and podfilling. Up to 47 and 75 days, respectively, bean \{LAI\} and pod dry weight were nearly identical in the two plant types in monoculture, after which growth ceased in the bush bean but continued in the climbibg beam. The 660 kg/ha yield advantage of the climbing bean over bush bean (3020 vs. 2360 kg/ha) was a direct result of podfilling after 75 sdays, which was supported in part by leaf area produced after 47 days. The large climbing bean yield loss when intercropped with maize (reduction of 1440 kg/ha in the bush bean vs. 1960 kg/ha in climbing bean) was attributed to greater overlap with the increasingly dominant maize. In climbing bean, maize caused two-week reductions in both the leaf area establishment and podfilling phases. When beans were intercropped, the durations of both phases were equal in the bush and climbing cultivars, and as a result, final yields (920 vs. 1060 kg/ha, respectively), were not significantly different. Differences in the timing and magnitude of growth responses in the indeterminate bush and climbing beans are discussed in relation to selecting beans for intercropping with maize. "
}
@article{Morales200457,
title = "The ecology and epidemiology of whitefly-transmitted viruses in Latin America ",
journal = "Virus Research ",
volume = "100",
number = "1",
pages = "57 - 65",
year = "2004",
note = "Plant Virus Epidemiology: First steps into the new millennium ",
issn = "0168-1702",
doi = "http://dx.doi.org/10.1016/j.virusres.2003.12.014",
url = "http://www.sciencedirect.com/science/article/pii/S0168170203003812",
author = "Francisco J Morales and Peter G Jones",
keywords = "Begomoviruses",
keywords = "Geminiviruses",
keywords = "Model",
keywords = "Probability surface",
keywords = "Cluster",
keywords = "Prediction",
keywords = "Integrated control ",
abstract = "Whitefly-transmitted geminiviruses are the most important constraint to the production of common bean and vegetable crops in the tropical lowlands and mid-altitude valleys of Latin America. Currently, over 30 distinct species of geminiviruses that are transmitted by the whitefly Bemisia tabaci infect common bean, tomato, pepper, cucurbits and other horticultural crops. A climate probability model (using FloraMap) was obtained using data from 304 geo-referenced locations where B. tabaci and geminiviruses cause significant damage. Clustering of the 304 points produced a simple model with two climatic variables: a dry season of at least 4 months each with less than 80 mm of rain and a mean temperature of the hottest month exceeding 21 °C. A modified Koeppen climate classification showed that 55% of the geminivirus-affected localities are in the tropical wet/dry region, 22% in the tropical and subtropical dry/humid climates and the remaining locations belonged to the wet equatorial and trade wind litoral climates. These findings contribute to understanding whitefly/geminivirus epidemics and implementation of sustainable integrated pest and disease management practices. "
}
@article{Johnson2003277,
title = "The distribution of benefits from public international germplasm banks: the case of beans in Latin America ",
journal = "Agricultural Economics ",
volume = "29",
number = "3",
pages = "277 - 286",
year = "2003",
note = "Returns to investment in plant genetic resource conservation and crop improvement research ",
issn = "0169-5150",
doi = "http://dx.doi.org/10.1016/S0169-5150(03)00055-0",
url = "http://www.sciencedirect.com/science/article/pii/S0169515003000550",
author = "N.L. Johnson and D. Pachico and O. Voysest",
keywords = "Genetic resource conservation",
keywords = "Crop varietal improvement",
keywords = "Beans",
keywords = "Latin America ",
abstract = "The unrestricted international flow of genetic resources from international genebank collections is the source of perhaps one of the greatest impacts of international agricultural research. This paper examines the distribution across countries in Latin America of benefits generated from bean genetic resources held by the International Center for Tropical Agriculture (CIAT). The genealogies of commercial bean cultivars released since 1976, containing materials from the collection, are analysed to calculate for each country the source of the genetic resources used. All countries in the region are shown to be heavily dependent on imported genetic resources for their commercial cultivars. From the available information on the economic impact of these improved bean varieties, the share of economic productivity benefits associated with imported germplasm by country of origin is calculated. The benefits received by each country from improved bean germplasm are compared with the contribution of that country’s germplasm to other countries. Some of the patterns in the flow and use of genetic resources are analysed. "
}
@article{Morales2001825,
title = "Conventional breeding for resistance to Bemisia tabaci-transmitted geminiviruses ",
journal = "Crop Protection ",
volume = "20",
number = "9",
pages = "825 - 834",
year = "2001",
note = "Challenges and Opportunities for Pest Management of Bemisia tabac i in the New Century ",
issn = "0261-2194",
doi = "http://dx.doi.org/10.1016/S0261-2194(01)00114-4",
url = "http://www.sciencedirect.com/science/article/pii/S0261219401001144",
author = "Francisco J. Morales",
keywords = "Bemisia tabaci",
keywords = "Bemisia argentifolii",
keywords = "Begomoviruses",
keywords = "Genetic resistance ",
abstract = "The whitefly Bemisia tabaci Genn. is a pervasive pest and vector of plant viruses. The existence of B. tabaci biotypes and numerous whitefly-transmitted geminiviruses (begomoviruses) affecting food and industrial crops has become a major constraint to agricultural development in tropical and subtropical regions of the world. The predominant whitefly and begomovirus method of control has been the application of insecticides. The excessive use of agrochemicals over the past decades, has contributed to an exponential increase in B. tabaci populations and incidence of begomoviruses transmitted by this whitefly vector. Under these conditions, biological and integrated whitefly/geminivirus control practices have not met expectations. Incorporating begomovirus resistance in a relatively small number of crops improved by conventional plant breeding methods has been a sustainable and efficient disease control strategy. This review discusses some of the conventional intra- and interspecific hybridization strategies followed to incorporate genetic resistance to begomoviruses in three major crops: cassava, common bean and tomato. "
}
@article{Díaz2010595,
title = "Concentrations of condensed tannins and anthocyanins in common bean seed coats ",
journal = "Food Research International ",
volume = "43",
number = "2",
pages = "595 - 601",
year = "2010",
note = "Molecular, Functional and Processing Characteristics of Whole Pulses and Pulse Fractions and their Emerging Food and Nutraceutical Applications ",
issn = "0963-9969",
doi = "http://dx.doi.org/10.1016/j.foodres.2009.07.014",
url = "http://www.sciencedirect.com/science/article/pii/S0963996909002221",
author = "Aura M. Díaz and Gina V. Caldas and Mathew W. Blair",
keywords = "Phaseolus vulgaris",
keywords = "Condensed tannins",
keywords = "Anthocyanins",
keywords = "Tannin monomer units ",
abstract = "Seed coat colour in common bean (Phaseolus vulgaris L.) is determined by activity of the flavonoid biosynthetic pathway resulting in the presence or absence of specific anthocyanins, tannins and glycosidic flavanols. These secondary metabolites have anti-oxidant properties in the case of anthocyanins and glycosidic flavonols and strongly influence dietary mineral bioavailability in the case of tannins. The modification of tannin content is a goal of biofortification breeding programs, while almost all bean improvement considers seed colour in selection priorities as this affects consumer preference and food quality. In the present study, we analyzed condensed tannins, tannin monomers and anthocyanin levels in an inter-genepool population derived from the cross \{DOR364\} × \{G19833\} using \{HPLC\} and spectrophotometric methods. The overall average for condensed tannins expressed as percentage in seed coats was 20.04%. The ranges were between 8.0% and 27.9% for soluble tannins (ST), 1.5% and 5.4% for insoluble tannins (IT), and 10.7% and 30.9% for total tannins (TT). Anthocyanins in seed coats averaged 0.08% (0.013–0.21% range) expressed as delphinidin-3-glucoside equivalents for the population with the distribution biased towards low content. All traits had large variability between genotypes and showed transgressive segregation, indicating quantitative inheritance for tannin content and oligogenic control of anthocyanins. Condensed tannins in the genotypes were mainly composed of catechin (60.3%), gallocatechin (25%), and afzelechin (14.7%) as monomeric units. "
}
