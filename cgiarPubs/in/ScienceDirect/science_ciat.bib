doi = "http://dx.doi.org/10.1016/j.jfoodeng.2008.10.034",
url = "http://www.sciencedirect.com/science/article/pii/S0260877408005359",
author = "A. Bechoff and D. Dufour and C. Dhuique-Mayer and C. Marouzé and M. Reynes and A. Westby",
