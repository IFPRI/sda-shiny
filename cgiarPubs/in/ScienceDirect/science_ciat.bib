url = "http://www.sciencedirect.com/science/article/pii/S0038071713000837",
author = "Delphine Renard and Jago Jonathan Birk and Anne Zangerlé and Patrick Lavelle and Bruno Glaser and Rumsaïs Blatrix and Doyle McKey",
keywords = "Anthrosol",
