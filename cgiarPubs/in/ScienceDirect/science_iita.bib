keywords = "Gene flow (outcrossing) between rice and weedy rice",
keywords = "Genetics",
keywords = "Herbicide-resistant rice cultivars",
keywords = "Herbicide-resistant weedy rice",
keywords = "Integrated weed management (IWM)",
keywords = "Irrigation and water management",
keywords = "Mitigation of gene flow (biotechnology, transgenic approaches)",
