@article{Niere2014313,
title = "Performance of tissue-cultured versus sucker-derived East African highland banana (Musa AAA-EA) under high and low input systems in Uganda ",
journal = "Field Crops Research ",
volume = "156",
number = "",
pages = "313 - 321",
