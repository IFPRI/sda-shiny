doi = "http://dx.doi.org/10.1016/j.cropro.2007.08.002",
url = "http://www.sciencedirect.com/science/article/pii/S0261219407001871",
author = "A. Ngakou and M. Tamò and I.A. Parh and D. Nwaga and N.N. Ntonifor and S. Korie and C.L.N. Nebane",
