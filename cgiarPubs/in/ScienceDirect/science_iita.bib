note = "",
issn = "0167-8809",
doi = "http://dx.doi.org/10.1016/j.agee.2009.11.006",
url = "http://www.sciencedirect.com/science/article/pii/S0167880909003375",
author = "Juan Guillermo Cobo and Gerd Dercon and Georg Cadisch",
keywords = "Aggregation",
