@article{Baldwin198840,
title = "T-cell clones in immunoparasitology ",
journal = "Parasitology Today ",
volume = "4",
number = "2",
pages = "40 - 45",
year = "1988",
note = "",
issn = "0169-4758",
doi = "http://dx.doi.org/10.1016/0169-4758(88)90063-4",
url = "http://www.sciencedirect.com/science/article/pii/0169475888900634",
author = "C.L. Baldwin and B.M. Goddeeris and W.I. Morrison",
abstract = "Cell-mediated immunity (CMI) is important in many parasitic infections. Stimulation by parasite-specific antigens can induce clonal expansion of parasite-specific T-cells which may act by direct cytotoxic action or by indirect effects on other cells such as natural killer cells or antibody-producing B-cells (Box 1). In many cases however, the precise effector functions and the identity of antigens that elicit protective responses in parasitic infections are poorly defined. Analysis of proliferative and cytotoxic activities of subcloned cultures of T-cells stimulated with parasite antigens can provide some clues about the importance of CMI, but, as this review shows, much more precise information can be obtained by analysis of the response of cloned T-cell cultures. "
}
@article{Nantulya1986168,
title = "Immunological approaches to the control of animal trypanosomiasis ",
journal = "Parasitology Today ",
volume = "2",
number = "6",
pages = "168 - 173",
year = "1986",
note = "",
issn = "0169-4758",
doi = "http://dx.doi.org/10.1016/0169-4758(86)90148-1",
url = "http://www.sciencedirect.com/science/article/pii/0169475886901481",
author = "V.M. Nantulya",
abstract = "Control of African trypanosomiasis relies heavily on attempts to eliminate the tsetse fly vectors, and the use of trypanocidal drugs which are not entirely satisfactory. But the prospects for immunological control have a long history — since the last century, explorers and settlers in Africa have progressively exposed their horses and cattle to tsetse bites (a process known as ‘salting’) in an attempt to build up some degree of immunity to the disease. More recently, immunologists and biochemists have used modern techniques to explore the possibility of suitable vaccines. In this article, Vinand Nantuyla discusses the problems and possible contributions of the immunological approach. "
}
