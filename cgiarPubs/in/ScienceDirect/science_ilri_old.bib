url = "http://www.sciencedirect.com/science/article/pii/001448949190088E",
author = "Moses K. Limo and Wolf P. Voigt and Aloys G. Tumbo-Oeri and R.M. Njogu and Onesmo K. Ole-MoiYoi",
keywords = "Ectoparasite",
keywords = "Ixodes Rhipicephalus appendiculatus",
keywords = "Salivary gland",
