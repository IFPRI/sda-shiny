url = "http://www.sciencedirect.com/science/article/pii/016524279605578X",
author = "W.C. Davis and W.C. Brown and M.J. Hamilton and C.R. Wyatt and J.A. Orden and A.M. Khalid and J. Naessens",
keywords = "Monoclonal antibodies",
