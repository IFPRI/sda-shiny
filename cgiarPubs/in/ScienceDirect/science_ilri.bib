note = "",
issn = "0304-4017",
doi = "http://dx.doi.org/10.1016/j.vetpar.2004.11.026",
url = "http://www.sciencedirect.com/science/article/pii/S0304401704006028",
author = "G.R. Muraguri and A. McLeod and J.J. McDermott and N. Taylor",
keywords = "Longitudinal study",
keywords = "Calves",
keywords = "Vector-borne diseases",
