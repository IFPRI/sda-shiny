author = "Katherine A. Taylor and Vittoria Lutje and David Kennedy and Edith Authié and Alain Boulangé and Linda Logan-Henfrey and Benson Gichuki and George Gettinby",
keywords = "African trypanosomiasis",
keywords = "Trypanosoma congolense",
keywords = "bovine",
keywords = "trypanotolerant",
keywords = "Bicell",
keywords = "VSG",
keywords = "variable surface glycoprotein",
keywords = "CP, cysteine protease (congopain)",
keywords = "hsp70, heat shock protein 70 kDa",
keywords = "BiP, immunoglobulin heavy chain binding protein",
keywords = "C63, 63-kDa recombinant protein ofT. congolensehsp70/BiP",
keywords = "ASC, antibody-secreting cell",
keywords = "SC, secreting cell",
keywords = "PCV, packed cell volume",
keywords = "PSG, phosphate-buffered glucose",
keywords = "pi, postinfection",
keywords = "PBMC, peripheral blood mononuclear cells",
keywords = "LPS, lipopolysaccharide",
keywords = "MAbs, monoclonal antibodies",
keywords = "FACS, fluorescence-activated cell sorter",
