url = "http://www.sciencedirect.com/science/article/pii/S0014489404001262",
author = "M. Hernandez-Valladares and J. Naessens and S. Nagda and A.J. Musoke and P. Rihet and O.K. ole-MoiYoi and F.A. Iraqi",
keywords = "and Abbreviationspi",
