note = "Conversations Across the DivideThe Time and Place for Political Ecology: The Life-Work of Piers BlaikieBiocomplexity in Coupled Human-Natural Systems: The Study of Population-Environment Interactions ",
issn = "0016-7185",
doi = "http://dx.doi.org/10.1016/j.geoforum.2007.03.011",
url = "http://www.sciencedirect.com/science/article/pii/S0016718507000528",
author = "Jennifer M. Olson and Gopal Alagarswamy and Jeffrey A. Andresen and David J. Campbell and Amelie Y. Davis and Jianjun Ge and Marianne Huebner and Brent M. Lofgren and David P. Lusch and Nathan J. Moore and Bryan C. Pijanowski and Jiaguo Qi and Philip K. Thornton and Nathan M. Torbick and Jing Wang",
keywords = "Climate change",
keywords = "Land use",
keywords = "Africa",
keywords = "Modeling",
keywords = "Biocomplexity",
keywords = "Human–environment interaction ",
abstract = "The questions of how land use change affects climate, and how climate change affects land use, require examination of societal and environmental systems across space at multiple scales, from the global climate to regional vegetative dynamics to local decision making by farmers and herders. It also requires an analysis of causal linkages and feedback loops between systems. These questions and the conceptual approach of the research design of the Climate–Land Interaction Project (CLIP) are rooted in the classical human–environment research tradition in Geography. This paper discusses a methodological framework to quantify the two-way interactions between land use and regional climate systems, using ongoing work by a team of multi-disciplinary scientists examining climate–land dynamics at multiple scales in East Africa. East Africa is a region that is undergoing rapid land use change, where changes in climate would have serious consequences for people’s livelihoods, and requiring new coping and land use strategies. The research involves exploration of linkages between two important foci of global change research, namely, land use/land cover (LULC) and climate change. These linkages are examined through modeling agricultural systems, land use driving forces and patterns, the physical properties of land cover, and the regional climate. Both qualitative and quantitative methods are being used to illustrate a diverse pluralism in scientific discovery. "
}
@article{Stentiford2016336,
title = "Microsporidia – Emergent Pathogens in the Global Food Chain ",
journal = "Trends in Parasitology ",
volume = "32",
number = "4",
pages = "336 - 348",
year = "2016",
note = "",
issn = "1471-4922",
doi = "http://dx.doi.org/10.1016/j.pt.2015.12.004",
url = "http://www.sciencedirect.com/science/article/pii/S1471492215002652",
author = "G.D. Stentiford and J.J. Becnel and L.M. Weiss and P.J. Keeling and E.S. Didier and B.A.P. Williams and S. Bjornson and M.L. Kent and M.A. Freeman and M.J.F. Brown and E.R. Troemel and K. Roesel and Y. Sokolova and K.F. Snowden and L. Solter",
keywords = "zoonotic",
keywords = "phylogeny",
keywords = "immune-suppression",
keywords = "intensive rearing",
keywords = "aquaculture",
keywords = "farming ",
abstract = "Intensification of food production has the potential to drive increased disease prevalence in food plants and animals. Microsporidia are diversely distributed, opportunistic, and density-dependent parasites infecting hosts from almost all known animal taxa. They are frequent in highly managed aquatic and terrestrial hosts, many of which are vulnerable to epizootics, and all of which are crucial for the stability of the animal–human food chain. Mass rearing and changes in global climate may exacerbate disease and more efficient transmission of parasites in stressed or immune-deficient hosts. Further, human microsporidiosis appears to be adventitious and primarily associated with an increasing community of immune-deficient individuals. Taken together, strong evidence exists for an increasing prevalence of microsporidiosis in animals and humans, and for sharing of pathogens across hosts and biomes. "
}
