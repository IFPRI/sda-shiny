@article{Boonstra1992165,
title = "Aquifer tests with partially penetrating wells: theory and practice ",
journal = "Journal of Hydrology ",
volume = "137",
number = "1–4",
pages = "165 - 179",
year = "1992",
note = "",
issn = "0022-1694",
doi = "http://dx.doi.org/10.1016/0022-1694(92)90054-Y",
url = "http://www.sciencedirect.com/science/article/pii/002216949290054Y",
author = "J. Boonstra",
abstract = "Two solutions to the problem of groundwater flow to partially penetrating wells are presented. The first solution is the well-known and widely used Hantush modification of the Theis method. The second is based on the author's recent work; it consists of a matching procedure between the drawdowns observed in the field and the theoretical drawdowns found from the analysis. This solution is only possible when specially developed software is used. Both solutions are described and compared with a theoretical data set. The comparison reveals important sensitivities in the estimate of the thickness of the tested aquifer. The author is of the opinion that the application of the Theis-Hantush method may lead to a serious overestimate of the aquifer thickness. These sensitivities are also illustrated using data from a field test in Pakistan. "
}
