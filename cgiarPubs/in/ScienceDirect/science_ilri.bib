pages = "197 - 213",
year = "1999",
note = "",
issn = "0304-4017",
doi = "http://dx.doi.org/10.1016/S0304-4017(98)00209-X",
url = "http://www.sciencedirect.com/science/article/pii/S030440179800209X",
author = "S.K Moloo and G.O Orinda and C.L Sabwa and S.H Minja and R.A Masake",
keywords = "Reservoir hosts",
keywords = "Buffalo",
keywords = "Eland",
keywords = "Waterbuck",
keywords = "Cattle-protozoa",
