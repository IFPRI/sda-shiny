doi = "http://dx.doi.org/10.1016/j.molbiopara.2009.09.009",
url = "http://www.sciencedirect.com/science/article/pii/S0166685109002400",
author = "Wannaporn Ittiprasert and Andre Miller and Jocelyn Myers and Vish Nene and Najib M. El-Sayed and Matty Knight",
keywords = "Juvenile Biomphalaria glabrata",
