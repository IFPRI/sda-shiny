url = "http://www.sciencedirect.com/science/article/pii/B9780444525123000231",
author = "S. Franzel and E. Kiptot and B. Lukuyu",
keywords = "Cattle",
keywords = "Dairy farmers",
keywords = "Extension",
keywords = "Feeding systems",
keywords = "Fodder",
keywords = "Forage",
